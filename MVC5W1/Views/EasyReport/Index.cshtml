@model IEnumerable<MVC5W1.Models.View_1>

@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.客戶名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.客戶銀行數)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.聯絡人數)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.客戶名稱)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.客戶銀行數)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.聯絡人數)
        </td>
    </tr>
}

</table>
